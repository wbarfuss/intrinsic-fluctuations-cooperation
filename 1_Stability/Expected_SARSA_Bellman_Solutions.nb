    RowBox[{"epsilon", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "1"}], "+", "s", "+", "t"}], ")"}]}]}]}]], "Input",
  CellChangeTimes->{{3.876800011667262*^9, 3.876800018590081*^9}, {
    3.876800050540906*^9, 3.8768000508699217`*^9}, {3.8768000840687637`*^9, 
